<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="ServiceSoap" />
            </basicHttpBinding>
            <customBinding>
                <binding name="ServiceSoap12">
                    <textMessageEncoding messageVersion="Soap12" />
                    <httpTransport />
                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="http://venus.eas.asu.edu/WSRepository/Services/Encryption/Service.asmx"
                binding="basicHttpBinding" bindingConfiguration="ServiceSoap"
                contract="Encryption.ServiceSoap" name="ServiceSoap" />
            <endpoint address="http://venus.eas.asu.edu/WSRepository/Services/Encryption/Service.asmx"
                binding="customBinding" bindingConfiguration="ServiceSoap12"
                contract="Encryption.ServiceSoap" name="ServiceSoap12" />
        </client>
    </system.serviceModel>
</configuration>